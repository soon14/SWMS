<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.swms.user.repository.mapper.RenderTemplateMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.swms.user.repository.entity.RenderTemplate">
        <id column="id" property="id"/>
        <result column="create_user" property="createUser"/>
        <result column="gmt_created" property="gmtCreated"/>
        <result column="modified_user" property="modifiedUser"/>
        <result column="gmt_modified" property="gmtModified"/>
        <result column="name" property="name"/>
        <result column="code" property="code"/>
        <result column="validate_schema" property="validateSchema"/>
        <result column="system_code" property="systemCode"/>
        <result column="description" property="description"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , create_user_id, gmt_created,modified_user_id, gmt_modified,
        `name`,code,validate_schema,system_code,description
    </sql>

</mapper>
